<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    app:startDestination="@id/login_page_navigation_fragment"
    android:id="@+id/pre_login_navigation">

    <fragment android:name="com.didebbo.mappify.presentation.view.fragment.destination.prelogin.LoginPage"
        android:id="@+id/login_page_navigation_fragment"
        android:label="Login"
        tools:layout="@layout/login_page_layout">
    </fragment>

    <fragment android:name="com.didebbo.mappify.presentation.view.fragment.destination.prelogin.RegisterPage"
        android:id="@+id/register_page_navigation_fragment"
        android:label="Register"
        tools:layout="@layout/register_page_layout">
    </fragment>

    <activity android:name="com.didebbo.mappify.presentation.view.activity.PostLoginActivity"
        android:id="@+id/post_login_activity"
        tools:layout="@layout/map_view_layout">
    </activity>


</navigation>